(function () {
    var brandhubFrontEndJsUrl = document.body.getAttribute('data-brandhub-frontend-js-url');
    if (brandhubFrontEndJsUrl) {
        var s = document.createElement('script');
        s.setAttribute('type', 'text/javascript');
        s.setAttribute('src', brandhubFrontEndJsUrl);
        document.body.appendChild(s);
    }
})();
